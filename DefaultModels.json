{
    "DefaultModels": {
        "all": {
            "*": "model1", // "*" for all player 
            "@vip/flag1": "model2", // select permission flag
            "#vip/group1": "model3", // select permission group
            "76561199XXXXXXXXXXXX": "model4" // select steamid
            // with priority: steamid > perm group > perm flag > all  
        },
        "t": {
            // t side config, will overwrite "all" config
        }
    },
    "WhoCanChangeModel": ["*"] // when it set to ["*"], this will be a default model feature
    // when it set to [], this will be a force model feature
}